#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nodejs/QuadB-Tech-assignment/node_modules/.pnpm/license@1.0.3/node_modules/license/dist/cli/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/.pnpm/license@1.0.3/node_modules/license/dist/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/.pnpm/license@1.0.3/node_modules/license/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/.pnpm/license@1.0.3/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/.pnpm/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules:/home/nodejs/node_modules:/home/node_modules:/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/license/dist/cli/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/license/dist/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/license/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/nodejs/QuadB-Tech-assignment/node_modules/.pnpm/license@1.0.3/node_modules/license/dist/cli/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/.pnpm/license@1.0.3/node_modules/license/dist/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/.pnpm/license@1.0.3/node_modules/license/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/.pnpm/license@1.0.3/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/.pnpm/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules:/home/nodejs/node_modules:/home/node_modules:/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/license/dist/cli/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/license/dist/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/license/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../license/dist/cli/index.js" "$@"
else
  exec node  "$basedir/../license/dist/cli/index.js" "$@"
fi
