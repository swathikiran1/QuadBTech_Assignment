#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nodejs/QuadB-Tech-assignment/node_modules/.pnpm/uuid@9.0.1/node_modules/uuid/dist/bin/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/.pnpm/uuid@9.0.1/node_modules/uuid/dist/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/.pnpm/uuid@9.0.1/node_modules/uuid/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/.pnpm/uuid@9.0.1/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/.pnpm/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules:/home/nodejs/node_modules:/home/node_modules:/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/uuid/dist/bin/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/uuid/dist/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/uuid/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/nodejs/QuadB-Tech-assignment/node_modules/.pnpm/uuid@9.0.1/node_modules/uuid/dist/bin/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/.pnpm/uuid@9.0.1/node_modules/uuid/dist/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/.pnpm/uuid@9.0.1/node_modules/uuid/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/.pnpm/uuid@9.0.1/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/.pnpm/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules:/home/nodejs/node_modules:/home/node_modules:/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/uuid/dist/bin/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/uuid/dist/node_modules:/home/nodejs/QuadB-Tech-assignment/node_modules/uuid/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../uuid/dist/bin/uuid" "$@"
else
  exec node  "$basedir/../uuid/dist/bin/uuid" "$@"
fi
